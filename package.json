{
	"name": "nepaltodayapp",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"start": "concurrently \"react-native start\"  \"react-native run-android --variant unsigneddebug\"",
		"log:android": "react-native log-android",
		"refresh": "adb shell input keyevent 46 46",
		"relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx",
		"reset": "react-native start --reset-cache",
		"jetify": "npx jetify",
		"build": "npx jetify && cd android && ./gradlew app:assembleSignedRelease && cd ..",
		"bundle": "npx jetify && cd android && ./gradlew app:bundleSignedRelease && cd ..",
		"test": "jest",
		"lint": "eslint ."
	},
	"dependencies": {
		"@eva-design/eva": "^1.4.0",
		"@react-native-community/async-storage": "^1.8.1",
		"@react-native-community/masked-view": "0.1.7",
		"@react-native-community/netinfo": "5.6.2",
		"@react-native-community/viewpager": "3.3.0",
		"appcenter": "^3.0.0",
		"appcenter-analytics": "^3.0.0",
		"appcenter-crashes": "^3.0.0",
		"format-number": "^3.0.0",
		"graphql": "^14.6.0",
		"jetifier": "^1.6.5",
		"moment": "2.24.0",
		"react": "16.13.0",
		"react-native": "0.61.5",
		"react-native-elements": "^1.2.1",
		"react-native-firebase": "5.6.0",
		"react-native-gesture-handler": "^1.6.0",
		"react-native-localize": "^1.3.4",
		"react-native-push-notification": "^3.1.9",
		"react-native-reanimated": "1.7.0",
		"react-native-safe-area-context": "0.7.3",
		"react-native-screens": "2.3.0",
		"react-native-scrollable-tab-view": "^1.0.0",
		"react-native-splash-screen": "^3.2.0",
		"react-native-svg": "12.0.3",
		"react-native-ui-kitten": "^4.4.1",
		"react-native-vector-icons": "^6.6.0",
		"react-native-webview": "^9.0.1",
		"react-navigation": "4.3.1",
		"react-navigation-stack": "2.3.1",
		"react-navigation-tabs": "2.8.4",
		"react-redux": "^7.2.0",
		"react-relay": "^9.0.0",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-saga": "^1.0.1",
		"relay-compiler": "^9.0.0",
		"relay-runtime": "^9.0.0",
		"styled-components": "^5.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.7",
		"@babel/runtime": "^7.8.7",
		"@react-native-community/eslint-config": "^0.0.7",
		"babel-eslint": "10.1.0",
		"babel-jest": "^25.1.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-relay": "^9.0.0",
		"babel-preset-react-app": "^9.1.1",
		"babel-preset-react-native": "^4.0.1",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "6.10.0",
		"eslint-config-standard": "14.1.0",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-node": "11.0.0",
		"eslint-plugin-prettier": "3.1.2",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-standard": "4.0.1",
		"jest": "^25.1.0",
		"metro-react-native-babel-preset": "^0.58.0",
		"prettier": "1.19.1",
		"react-native-dotenv": "0.2.0",
		"react-test-renderer": "16.13.0"
	},
	"jest": {
		"preset": "react-native",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
		},
		"transformIgnorePatterns": [
			"!node_modules/react-runtime"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/"
		]
	}
}
